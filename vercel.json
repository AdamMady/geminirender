{
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@20.x" // Try this first for Node.js 20
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api"
    }
  ],
  "build": {
    "env": {
      "GEMINI_MODEL_ID": "gemini-2.0-flash",
      "GENERATE_CONTENT_API": "streamGenerateContent"
    }
  }
}
